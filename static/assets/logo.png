<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 50" width="200" height="50">
  <defs>
    <linearGradient id="logoGradient" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#007bff"/>
      <stop offset="100%" stop-color="#28a745"/>
    </linearGradient>
  </defs>

  <!-- Stylized "V" with Neural Links -->
  <g stroke="url(#logoGradient)" stroke-width="3" fill="none" stroke-linecap="round">
    <path d="M10 10 L25 40 L40 10" />
    <!-- Neural Nodes -->
    <circle cx="10" cy="10" r="2" fill="#007bff"/>
    <circle cx="25" cy="40" r="2" fill="#28a745"/>
    <circle cx="40" cy="10" r="2" fill="#007bff"/>
    <line x1="10" y1="10" x2="25" y2="40" stroke-dasharray="2 2"/>
    <line x1="25" y1="40" x2="40" y2="10" stroke-dasharray="2 2"/>
  </g>

  <!-- Text -->
  <text x="55" y="30" font-size="20" font-family="'Nunito', sans-serif" fill="url(#logoGradient)">
    Vishnu AI
  </text>
</svg>
